[sub_resource type="Shader" id=5]
code = "shader_type canvas_item;

uniform float offset = 1.5f;

uniform float vignette_intensity = 0.05;
uniform float vignette_opacity : hint_range(0.0, 1.0) = 1;
uniform vec4 vignette_rgb : hint_color = vec4(0.0, 0.0, 0.0, 1.0);

float vignette(vec2 uv){
	uv *= 1.0 - uv.xy;
	float vignette = uv.x * uv.y * 20.0;
	return pow(vignette, vignette_intensity * vignette_opacity);
}

void fragment(){
	vec4 green_channel = texture(SCREEN_TEXTURE, SCREEN_UV);
	vec4 red_channel = texture(SCREEN_TEXTURE, vec2(SCREEN_UV.x + (offset * SCREEN_PIXEL_SIZE.x), SCREEN_UV.y));
	vec4 blue_channel = texture(SCREEN_TEXTURE, vec2(SCREEN_UV.x - (offset * SCREEN_PIXEL_SIZE.x), SCREEN_UV.y));
	
	vec4 text = texture(TEXTURE, UV);
	text.rgba *= (vignette_rgb.rgba);
	text.rgba *= (1.0 - vignette(UV));
	
	// COLOR += vec4((text.rgb)*color.rgb,text.a);
	COLOR = vec4(red_channel.r - text.a, green_channel.g - text.a, blue_channel.b - text.a, 1f);
}"

